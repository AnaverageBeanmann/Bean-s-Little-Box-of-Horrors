"VertexLitGeneric"
{
	"$basetexture" "models/vj_blboh/heavy/hvyweapon_red"
	"$bumpmap" "models/vj_blboh/heavy/hvyweapon_normal"

	"$yellow" "0"
	"$one" "1"

	"$halflambert" "0"

	"360?$color2" "[ 0.8 0.8 0.8 ]"

	"$sheenPassEnabled" "1"

	"$sheenmap" 		"models/vj_blboh/heavy/cubemap_sheen001"
	"$sheenmapmask" 		"models/vj_blboh/heavy/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"spy_invis" //wmodel and team color cloak support
		{
		}
		"invis" //vmodel cloak support
		{
		}
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Equals"
		{
			"srcVar1" "$yellow"
			"resultVar" "$color2"
		}
	}
}